{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach by Process ID",
			"processId": "${command:PickProcess}",
			"skipFiles": [
				"<node_internals>/**"
			],
			"restart": true,
			"smartclientBin": "C:/totvs/bin/smartclient/smartclient.exe"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}\\web",
			"smartclientBin": "C:/totvs/bin/smartclient/smartclient.exe"
		}
	]
}
